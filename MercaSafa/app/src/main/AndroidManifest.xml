<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mercasafa">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/safa_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MercaSafa"
        >
        <activity android:name=".PantallaDetalleSeleccionPersonal"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".pantallaSeleccionPersonal" android:screenOrientation="portrait"/>
        <activity android:name=".detalleObjeto" android:screenOrientation="portrait"/>
        <activity android:name=".nuevoObjeto" android:screenOrientation="portrait"/>
        <activity android:name=".pantallaSeleccion" android:screenOrientation="portrait"/>
        <activity android:name=".Menu" android:screenOrientation="portrait"/>
        <activity android:name=".PantallaIniciarSesion" android:screenOrientation="portrait"/>
        <activity android:name=".PantallaRegistro" android:screenOrientation="portrait" />
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>